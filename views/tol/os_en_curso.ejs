<% if (data.length > 0) { %>
    <table class="table">
        <thead>
          <tr>
            <th scope="col">Orden de servicio</th>
            <th scope="col">Fecha creaci√≥n</th>
            <th scope="col">Fecha entrega</th>
            <th scope="col">Detalle</th>
            
          </tr>
        </thead>
        <tbody>
    <% for (let rows of data) { %>
        <tr>
            <td scope="row"><p><%= rows.orden %></p></td>
            <td><p><%= rows.fecha_creacion %></p></td>
            <td><p><%= rows.fecha_entrega %></p></td>
            <td>
                <a class="btn btn-primary" type="button" href="/tol/os_en_curso/<%= rows.orden %>">Detalle</a>
            </td>
            
        </tr>

    <% } %>  
    </tbody>
    </table>  
<% } else { %> 
